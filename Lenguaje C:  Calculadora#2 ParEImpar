#include <stdio.h>

int main() {
    int num = 0, numero;

    printf("Ingrese 10 n√∫meros:\n");

    do {
        num++;
        scanf("%d", &numero);

        if (numero % 2 == 0) {
            printf("Es Par\n");
        } else {
            printf("Es Impar\n");
        }
    } while (num < 10);

    return 0;
}

